{
  "name": "CyberBoB",
  "description": "A cactus in cyberspace",
  "author": "Jens C. <jca@osaft.org>",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
      "default_title": "Doing things only a cactus can do",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
